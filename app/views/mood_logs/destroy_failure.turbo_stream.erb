<% if params[:from] == "edit" %>
  <%= turbo_stream.update "modal-content" do %>
    <%= render "mood_logs/partials/modal_form", mood_log: @mood_log, errors: true  %>
  <% end %>

<% elsif params[:from] == "show" %>
  <%= turbo_stream.update "modal-content" do %>
    <%= render "mood_logs/partials/modal_show", mood_log: @mood_log, errors: true  %>
  <% end %>
<% end %>
